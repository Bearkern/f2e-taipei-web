<div class="row"> 
<%for(var i=0;i<data.length;i++) {%>
	<div class="col-sm-6 col-md-4">
		<div class="thumbnail"> 
			<img src="<%=data[i].image%>" data-holder-rendered="true" style="height: 240px; width: 170px; display: block;"> 
			<div class="caption"> 
				<h3><%=data[i].title%></h3> 
				<p><%=data[i].content%></p> 
				<p>
					<a href="<%=data[i].link.url%>" class="btn btn-primary" role="button" target="_blank"><%=data[i].link.name%></a> 
				</p> 
			</div> 
		</div> 
	</div> 
<%}%>	
